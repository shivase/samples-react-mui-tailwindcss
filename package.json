{
  "name": "materialui-the-complete-guide-with-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "postinstall": "typesync",
    "dev": "vite",
    "build": "rimraf dist && tsc && NODE_ENV=production vite build",
    "preview": "vite preview",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,html}'",
    "lint:prettier": "prettier --no-error-on-unmatched-pattern --write src/**/*.{js,jsx,ts,tsx,html}",
    "lint:css": "stylelint src/**/*.{css,scss,sass}",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@headlessui/react": "^1.7.13",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@mui/system": "^5.11.13",
    "@mui/x-date-pickers": "^6.0.2",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-query-devtools": "^4.26.1",
    "@types/react-transition-group": "^4.4.5",
    "axios": "^1.3.4",
    "clsx": "^1.2.1",
    "firebase": "^9.18.0",
    "nanoid": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-icons": "^4.8.0",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "react-transition-group": "^4.4.5",
    "tailwindcss-debug-screens": "^2.2.1",
    "zod": "^3.21.4",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@eslint/eslintrc": "^2.0.1",
    "@mswjs/data": "^0.11.2",
    "@storybook/react": "^6.5.16",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "^8.21.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "eslint-plugin-testing-library": "^5.10.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.0",
    "msw": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "rimraf": "^4.4.0",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-scss": "^4.5.0",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "typesync": "^0.10.0",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.29.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "src/**/*.{ts,js,jsx,tsx,json,html}": [
      "prettier --write --loglevel=error",
      "eslint --fix"
    ],
    "src/**/*.{css,scss,sass}": [
      "stylelint"
    ]
  }
}
